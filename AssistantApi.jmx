        <JSR223Sampler guiclass="TestBeanGUI" testclass="JSR223Sampler" testname="Assistant_api_data">
          <stringProp name="cacheKey">true</stringProp>
          <stringProp name="filename"></stringProp>
          <stringProp name="parameters"></stringProp>
          <stringProp name="script">import org.apache.commons.lang3.RandomStringUtils

// Generate unique name &amp; description per thread with &quot;QAA&quot; appended
def uniqueProjectName = RandomStringUtils.randomAlphabetic(10).toLowerCase() + &quot; QAA&quot;
def uniqueProjectDesc = RandomStringUtils.randomAlphabetic(10).toLowerCase() + &quot; QAA&quot;
def uniqueResourceName = RandomStringUtils.randomAlphabetic(10).toLowerCase() + &quot; QAA&quot;
def uniqueResourceDesc = RandomStringUtils.randomAlphabetic(10).toLowerCase() + &quot; QAA&quot;
def uniqueConversationTitle = RandomStringUtils.randomAlphabetic(10).toLowerCase() + &quot; QAA&quot;

// Store in variable
vars.put(&quot;project_name&quot;, uniqueProjectName)
vars.put(&quot;project_desc&quot;, uniqueProjectDesc)

vars.put(&quot;resource_name&quot;, uniqueResourceName)
vars.put(&quot;resource_desc&quot;, uniqueResourceDesc)

vars.put(&quot;title_name&quot;, uniqueConversationTitle)

// Generate unique updated name &amp; description per thread with &quot;QAA&quot; appended
def updatedProjectName = RandomStringUtils.randomAlphabetic(10).toLowerCase() + &quot; QAA&quot;
def updatedProjectDesc = RandomStringUtils.randomAlphabetic(10).toLowerCase() + &quot; QAA&quot;
def updatedCoversationtitle = RandomStringUtils.randomAlphabetic(10).toLowerCase() + &quot; QAA&quot;
def updatedResourceDesc = RandomStringUtils.randomAlphabetic(10).toLowerCase() + &quot; QAA&quot;

// Store in variable
vars.put(&quot;updated_project_name&quot;, updatedProjectName)
vars.put(&quot;updated_project_desc&quot;, updatedProjectDesc)

vars.put(&quot;updated_conversation_title&quot;, updatedCoversationtitle)
vars.put(&quot;updated_resource_desc&quot;, uniqueResourceDesc)
</stringProp>
          <stringProp name="scriptLanguage">groovy</stringProp>
        </JSR223Sampler>
