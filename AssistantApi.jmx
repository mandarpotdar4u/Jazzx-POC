            <stringProp name="script">import groovy.json.JsonSlurper

def json = new JsonSlurper().parseText(prev.getResponseDataAsString())

def expectedPhrases = [
    &quot;Imaginary Inc. - Financial Report (Q1 2025)&quot;,
    &quot;Revenue: $1,250,000&quot;,
    &quot;Gross Profit: $550,000&quot;,
    &quot;Net Profit: $350,000&quot;,
    &quot;net margin of 28%&quot;,
    &quot;cost control measures&quot;
]

// Find the completed response with the full report
def fullContent = json.find { it.response_content?.contains(&quot;Imaginary Inc.&quot;) }?.response_content

// Assert each expected phrase
expectedPhrases.each { phrase -&gt;
    if (!fullContent?.contains(phrase)) {
        AssertionResult.setFailure(true)
        AssertionResult.setFailureMessage(&quot;Missing expected phrase: &apos;$phrase&apos;&quot;)
    }
}
